{
  "name": "cloud-backend",
  "version": "1.0.0",
  "description": "Cloud Backend",
  "main": "dist/index.js",
  "scripts": {
    "watch": "tsc -w",
    "dev": "nodemon .",
    "build": "rm -r dist/; tsc",
    "start": "node .",
    "auth": "node dist/auth.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run"
  },
  "keywords": [],
  "author": "Deramecourt Mathurin",
  "license": "ISC",
  "dependencies": {
    "apollo-server-core": "^3.6.4",
    "apollo-server-express": "^3.6.3",
    "argon2": "^0.28.4",
    "connect-redis": "^6.1.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "graphql": "^15.8.0",
    "ioredis": "^4.28.5",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.18",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/node": "^17.0.17",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
